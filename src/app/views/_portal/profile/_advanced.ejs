<div class="md-padding">
    <md-card class="md-whiteframe-3dp">
        <section>
            <md-subheader class="md-primary md-no-sticky">
                <!--<i class="zmdi zmdi-face zmdi-hc-2x md-padding"></i>-->
                Advanced
            </md-subheader>
            <span layout="row" class="md-block" layout-align="stretch" layout-md="column" layout-sm="column"
                  layout-xs="column">
                <md-button ng-click="enableAPI()" disabled flex class="md-primary">
                    Enable API Access (Unavailable)
                </md-button>
            </span>
            <span layout="row" class="md-block" layout-align="stretch" layout-md="column" layout-sm="column"
                  layout-xs="column">
                <md-button ng-click="requestData()" disabled flex class="md-primary">
                    Download Account Data (Unavailable)
                </md-button>
            </span>
            <span layout="row" class="md-block" layout-align="stretch" layout-md="column" layout-sm="column"
                  layout-xs="column">
                <md-button ng-click="deleteAccount()" flex class="md-warn">
                    Delete Account
                </md-button>
            </span>
        </section>
    </md-card>
</div>
